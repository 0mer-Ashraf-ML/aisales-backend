{
    "info": {
      "name": "Company and Prospect API",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
      "description": "Collection for managing companies and prospects"
    },
    "item": [
      {
        "name": "Authentication",
        "item": [
          {
            "name": "Login",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/auth/login",
                "host": ["{{baseUrl}}"],
                "path": ["auth", "login"]
              },
              "description": "Login to get JWT token for authentication"
            }
          }
        ]
      },
      {
        "name": "Companies",
        "item": [
          {
            "name": "Get All Companies",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/companies",
                "host": ["{{baseUrl}}"],
                "path": ["companies"]
              },
              "description": "Get all companies for the authenticated user"
            }
          },
          {
            "name": "Get Company by ID",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/companies/{{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["companies", "{{companyId}}"]
              },
              "description": "Get a company by its ID"
            }
          },
          {
            "name": "Create Company",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                },
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"company_name\": \"Example Company\",\n  \"dba\": \"EC\",\n  \"products_services\": [\"SaaS\", \"Consulting\"],\n  \"buyer_industries\": [\"Technology\", \"Finance\"],\n  \"web_url\": \"www.example.com\",\n  \"target_region\": [\"NAM\", \"EMEA\"],\n  \"target_industries\": [\"Healthcare\", \"Education\"],\n  \"preferred_company_size\": \"200\",\n  \"preferred_contact_department\": [\"IT\", \"Sales\"],\n  \"preferred_industry_keywords\": [\"Cloud\", \"Security\"],\n  \"tech_stack\": [\"AWS\", \"React\"]\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/companies",
                "host": ["{{baseUrl}}"],
                "path": ["companies"]
              },
              "description": "Create a new company"
            }
          },
          {
            "name": "Update Company",
            "request": {
              "method": "PATCH",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                },
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"company_name\": \"Updated Company Name\",\n  \"web_url\": \"www.updated-example.com\"\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/companies/{{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["companies", "{{companyId}}"]
              },
              "description": "Update an existing company"
            }
          },
          {
            "name": "Delete Company",
            "request": {
              "method": "DELETE",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/companies/{{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["companies", "{{companyId}}"]
              },
              "description": "Delete a company by its ID"
            }
          },
          {
            "name": "Import Company with Prospects",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                },
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"company_name\": \"oakAI\",\n  \"dba\": \"oakAI\",\n  \"products_services\": [\n    \"SaaS\"\n  ],\n  \"buyer_industries\": [\n    \"IT\",\n    \"AI\"\n  ],\n  \"web_url\": \"www.oakai.com\",\n  \"target_region\": [\n    \"NAM\",\n    \"Asia\"\n  ],\n  \"target_industries\": [\n    \"Technology\",\n    \"Software\"\n  ],\n  \"preferred_company_size\": \"500\",\n  \"preferred_contact_department\": [\n    \"Head of Engineering\",\n    \"CEO\",\n    \"CTO\"\n  ],\n  \"preferred_industry_keywords\": [\n    \"AI\",\n    \"SaaS\",\n    \"LLM\"\n  ],\n  \"tech_stack\": [\n    \"AI\",\n    \"SaaS\",\n    \"LLM\"\n  ],\n  \"prospects\": [\n    {\n      \"country\": \"China\",\n      \"company_name\": \"金叶天成（北京）科技有限公司\",\n      \"company_website\": null,\n      \"industry\": \"Technology\",\n      \"num_employees\": 26,\n      \"annual_revenue\": 110700000,\n      \"name\": \"田立军\",\n      \"title\": \"CTO\",\n      \"recommended_email\": null,\n      \"emails\": [],\n      \"phones\": [],\n      \"company_keyword\": [],\n      \"linkedin_url\": \"https://www.linkedin.com/in/立军-田-255aa4b5\"\n    },\n    {\n      \"country\": \"China\",\n      \"company_name\": \"北京智眸科技有限公司\",\n      \"company_website\": null,\n      \"industry\": \"Technology\",\n      \"num_employees\": 26,\n      \"annual_revenue\": 110700000,\n      \"name\": \"鲁耀杰\",\n      \"title\": \"CTO\",\n      \"recommended_email\": null,\n      \"emails\": [],\n      \"phones\": [],\n      \"company_keyword\": [],\n      \"linkedin_url\": \"https://www.linkedin.com/in/luyaojie\"\n    }\n  ]\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/companies/import",
                "host": ["{{baseUrl}}"],
                "path": ["companies", "import"]
              },
              "description": "Import a company with its prospects in a single request"
            }
          }
        ]
      },
      {
        "name": "Prospects",
        "item": [
          {
            "name": "Get All Prospects for Company",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/prospects?companyId={{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["prospects"],
                "query": [
                  {
                    "key": "companyId",
                    "value": "{{companyId}}"
                  }
                ]
              },
              "description": "Get all prospects for a specific company"
            }
          },
          {
            "name": "Get Prospect by ID",
            "request": {
              "method": "GET",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/prospects/{{prospectId}}?companyId={{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["prospects", "{{prospectId}}"],
                "query": [
                  {
                    "key": "companyId",
                    "value": "{{companyId}}"
                  }
                ]
              },
              "description": "Get a prospect by its ID"
            }
          },
          {
            "name": "Create Prospect",
            "request": {
              "method": "POST",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                },
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"name\": \"John Doe\",\n  \"title\": \"CTO\",\n  \"country\": \"United States\",\n  \"company_name\": \"ABC Corp\",\n  \"company_website\": \"www.abccorp.com\",\n  \"industry\": \"Technology\",\n  \"recommended_email\": \"john.doe@abccorp.com\",\n  \"emails\": [\"john.doe@abccorp.com\", \"jdoe@abccorp.com\"],\n  \"phones\": [\"+1234567890\"],\n  \"company_keyword\": [\"AI\", \"SaaS\"],\n  \"linkedin_url\": \"https://www.linkedin.com/in/johndoe\",\n  \"num_employees\": 500,\n  \"annual_revenue\": 10000000,\n  \"company_id\": \"{{companyId}}\"\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/prospects",
                "host": ["{{baseUrl}}"],
                "path": ["prospects"]
              },
              "description": "Create a new prospect"
            }
          },
          {
            "name": "Update Prospect",
            "request": {
              "method": "PATCH",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                },
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\n  \"name\": \"John Smith\",\n  \"title\": \"Chief Technology Officer\",\n  \"emails\": [\"john.smith@abccorp.com\", \"jsmith@abccorp.com\"]\n}"
              },
              "url": {
                "raw": "{{baseUrl}}/prospects/{{prospectId}}?companyId={{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["prospects", "{{prospectId}}"],
                "query": [
                  {
                    "key": "companyId",
                    "value": "{{companyId}}"
                  }
                ]
              },
              "description": "Update an existing prospect"
            }
          },
          {
            "name": "Delete Prospect",
            "request": {
              "method": "DELETE",
              "header": [
                {
                  "key": "Authorization",
                  "value": "Bearer {{authToken}}"
                }
              ],
              "url": {
                "raw": "{{baseUrl}}/prospects/{{prospectId}}?companyId={{companyId}}",
                "host": ["{{baseUrl}}"],
                "path": ["prospects", "{{prospectId}}"],
                "query": [
                  {
                    "key": "companyId",
                    "value": "{{companyId}}"
                  }
                ]
              },
              "description": "Delete a prospect by its ID"
            }
          }
        ]
      }
    ],
    "event": [
      {
        "listen": "prerequest",
        "script": {
          "type": "text/javascript",
          "exec": [""]
        }
      },
      {
        "listen": "test",
        "script": {
          "type": "text/javascript",
          "exec": [""]
        }
      }
    ],
    "variable": [
      {
        "key": "baseUrl",
        "value": "http://localhost:3000",
        "type": "string"
      },
      {
        "key": "authToken",
        "value": "your_jwt_token_here",
        "type": "string"
      },
      {
        "key": "companyId",
        "value": "company_uuid_here",
        "type": "string"
      },
      {
        "key": "prospectId",
        "value": "prospect_uuid_here",
        "type": "string"
      }
    ]
  }